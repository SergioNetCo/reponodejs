#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node" --stack_trace_limit=25 "$basedir/../move/bin/move" "$@"
else 
  exec node --stack_trace_limit=25 "$basedir/../move/bin/move" "$@"
fi
